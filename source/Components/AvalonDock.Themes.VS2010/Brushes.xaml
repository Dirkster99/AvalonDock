	<DrawingBrush
		x:Key="AvalonDock_ThemeVS2010_BaseColor1"
		Stretch="None"
		TileMode="Tile"
		Viewport="0 0 5 5"
		ViewportUnits="Absolute">
		<DrawingBrush.Drawing>
			<DrawingGroup>
				<GeometryDrawing Brush="#2C3D5A">
					<GeometryDrawing.Geometry>
						<RectangleGeometry Rect="0,0,10,10" />
					</GeometryDrawing.Geometry>
				</GeometryDrawing>
				<GeometryDrawing>
					<GeometryDrawing.Pen>
						<Pen Brush="#35496A" Thickness="1" />
					</GeometryDrawing.Pen>
					<GeometryDrawing.Geometry>
						<GeometryGroup>
							<LineGeometry StartPoint="0,0" EndPoint="10,10" />
						</GeometryGroup>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>
				<GeometryDrawing>
					<GeometryDrawing.Pen>
						<Pen Brush="#293955" Thickness="1" />
					</GeometryDrawing.Pen>
					<GeometryDrawing.Geometry>
						<GeometryGroup>
							<LineGeometry StartPoint="1,0" EndPoint="10,9" />
						</GeometryGroup>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>
			</DrawingGroup>
		</DrawingBrush.Drawing>
	</DrawingBrush>

